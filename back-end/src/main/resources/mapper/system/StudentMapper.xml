<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lqh.system.mapper.StudentMapper">
    <select id="getCourseCnameByStudentSno" parameterType="String" resultType="String">
        SELECT course.Cname
        FROM sc ,course
        WHERE sc.Cno = course.Cno
        AND sno = #{studentSno}
    </select>
</mapper>
